{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ],
    "quotemark": false,
    "no-trailing-whitespace": false,
    {
      "trailingComma": "es5",
      "tabWidth": 4,
      "semi": true,
      "singleQuote": true
    }
  }
}
